{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/breadcrumb/toc.yml",
      "extendBreadcrumb": true
    },
    "fileMetadata": {
      "ms.topic":
       {
         "articles/**.md": "reference",
         "conceptuals/**.md": "conceptual"
       }
    },
    "template": [],
    "dest": "VBA-Docs",
    "markdownEngineName": "markdig"
  }
}
